<!DOCTYPE Linker_Placement_File>
<Root name="Flash Section Placement">
<MemorySegment name="$(FLASH_NAME:FLASH)">
<ProgramSection load="no" name=".reserved_flash" start="$(FLASH_PH_START)" size="$(FLASH_START)-$(FLASH_PH_START)" size_symbol="__m_sd_size"/>
<ProgramSection alignment="100" load="Yes" name=".vectors" start="$(FLASH_START:)"/>
<ProgramSection alignment="4" load="Yes" name=".init"/>
<ProgramSection alignment="4" load="Yes" name=".init_rodata"/>
<ProgramSection alignment="4" load="Yes" name=".text"/>
<ProgramSection alignment="4" load="Yes" name=".dtors"/>
<ProgramSection alignment="4" load="Yes" name=".ctors"/>
<ProgramSection alignment="4" load="Yes" name=".rodata"/>
<ProgramSection alignment="4" keep="Yes" load="Yes" name=".button_handlers" inputsections="*(.button_handlers*)" address_symbol="__start_button_handlers" end_symbol="__stop_button_handlers"/>
<ProgramSection alignment="4" load="Yes" name=".ARM.exidx" address_symbol="__exidx_start" end_symbol="__exidx_end"/>
<ProgramSection alignment="4" load="Yes" runin=".fast_run" name=".fast"/>
<ProgramSection alignment="4" load="Yes" runin=".data_run" name=".data"/>
<ProgramSection alignment="4" load="Yes" runin=".tdata_run" name=".tdata"/>
<ProgramSection alignment="4" keep="Yes" load="No" name=".nrf_sections" address_symbol="__start_nrf_sections" />
<ProgramSection alignment="4" keep="Yes" load="Yes" name=".fs_data" runin=".fs_data_run" inputsections="*(.fs_data*)" />
</MemorySegment>
<MemorySegment name="$(RAM_NAME:RAM);SRAM">
<ProgramSection load="no" name=".reserved_ram" start="$(RAM_PH_START)" size="$(RAM_START)-$(RAM_PH_START)" />
<ProgramSection alignment="100" load="No" name=".vectors_ram" start="$(RAM_START:$(SRAM_START:))"/>
<ProgramSection alignment="4" load="No" name=".fast_run"/>
<ProgramSection alignment="4" load="No" name=".data_run"/>
<ProgramSection alignment="4" load="No" name=".tdata_run"/>
<ProgramSection alignment="4" load="No" keep="Yes" name=".nrf_sections_run" address_symbol="__start_nrf_sections_run" />
<ProgramSection alignment="4" load="No" keep="Yes" name=".fs_data_run" address_symbol="__start_fs_data" end_symbol="__stop_fs_data"/>
<ProgramSection alignment="4" load="No" keep="Yes" name=".nrf_sections_run_end" address_symbol="__end_nrf_sections_run" />
<ProgramSection alignment="4" load="No" name=".bss"/>
<ProgramSection alignment="4" load="No" name=".tbss"/>
<ProgramSection alignment="4" load="No" name=".non_init"/>
<ProgramSection alignment="4" size="__HEAPSIZE__" load="No" name=".heap"/>
<ProgramSection alignment="8" size="__STACKSIZE__" load="No" place_from_segment_end="Yes" name=".stack"/>
<ProgramSection alignment="8" size="__STACKSIZE_PROCESS__" load="No" name=".stack_process"/>
</MemorySegment>
<MemorySegment name="$(FLASH2_NAME:FLASH2)">
<ProgramSection alignment="4" load="Yes" name=".text2"/>
<ProgramSection alignment="4" load="Yes" name=".rodata2"/>
<ProgramSection alignment="4" load="Yes" runin=".data2_run" name=".data2"/>
</MemorySegment>
<MemorySegment name="$(RAM2_NAME:RAM2)">
<ProgramSection alignment="4" load="No" name=".data2_run"/>
<ProgramSection alignment="4" load="No" name=".bss2"/>
</MemorySegment>
</Root>
</Root>
